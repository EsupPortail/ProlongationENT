// NB: not pure json, comments & some trailing commas are allowed :)
{
    "theme": "theme-simple",
    "plugins": [],
    "cas_base_url": "https://cas.univ.fr/cas",
    "uportal_base_url": "https://ent.univ.fr",
    "prolongationENT_url": "http://localhost:8082/ProlongationENT",

    "layout_url": "https://ent.univ.fr/layout.jsp",

    // if you use EsupUserApps. You can comment "layout_url" above
    //"esupUserApps_url": "https://ent.univ.fr/EsupUserApps",

    "time_before_checking_browser_cache_is_up_to_date": 60,

    // disabling localStorage will cause late appearing of the prolongationENT => blinks in the browser
    "disableLocalStorage": false,
    // do disable server cache in production, 
    "disableServerCache": false,
    // to debug theme CSS in browser, it's easier to load directly the CSS
    "disableCSSInlining": false,
}
